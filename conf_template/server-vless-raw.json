{
  "remarks": "SERVER",
  "log": {
    "dnsLog": false,
    "loglevel": "warning",
    "access": "./access.log",
    "error": "./error.log",
    "maskAddress": ""
  },
  "dns": {
    "hosts": {
      "ntc.party": "2a02:e00:ffec:4b8::1"
    }
  },
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "domainMatcher": "hybrid",
    "network": "tcp,udp",
    "rules": [
      {
        "network": "tcp,udp",
        "port": 53,
        "outboundTag": "dns-out"
      },
      {
        "inboundTag": [
          "api"
        ],
        "outboundTag": "api"
      },
      {
        "protocol": [
          "bittorrent"
        ],
        "outboundTag": "direct"
      },
      {
        "ip": [
          "geoip:private"
        ],
        "outboundTag": "direct"
      },
      {
        "domain": [
          "keyword:remini"
        ],
        "outboundTag": "direct"
      },
      {
        "domain": [
          "geosite:category-ads-all",
          "keyword:analytics",
          "keyword:telemetry",
          "keyword:bongacams",
          "keyword:firebase",
          "keyword:tracking",
          "keyword:reklama"
        ],
        "outboundTag": "block"
      },
      {
        "domain": [
          "geosite:intel",
          "gemini.google.com"
        ],
        "outboundTag": "warp"
      },
      {
        "domain": [
          "geosite:meta"
        ],
        "outboundTag": "directIPv4"
      },
      {
        "domain": [
          "geosite:youtube",
          "geosite:google",
          "domain:ntc.party"
        ],
        "outboundTag": "directIPv6"
      },
      {
        "ip": [
          "geoip:ru"
        ],
        "outboundTag": "warp"
      }
    ]
  },
  "inbounds": [
    {
      "tag": "vless-in",
      "listen": "127.0.0.1",
      "port": 10550,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "email": "USERNAME_TEMP",
            "id": "UUID_TEMP"
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "tcp",
        "security": "none",
        "tcpSettings": {
          "acceptProxyProtocol": true,
          "header": {
            "type": "none"
          }
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls",
          "quic",
          "fakedns"
        ],
        "metadataOnly": false,
        "routeOnly": false
      },
      "allocate": {
        "strategy": "always",
        "refresh": 5,
        "concurrency": 3
      }
    },
    {
      "listen": "127.0.0.1",
      "port": 9953,
      "protocol": "dokodemo-door",
      "settings": {
        "address": "127.0.0.1"
      },
      "tag": "api"
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "userLevel": 0,
      "settings": {
        "domainStrategy": "UseIP"
      },
      "sockopt": {
        "tcpMaxSeg": 1492,
        "tcpFastOpen": true
      }
      "tag": "direct"
    },
    {
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIPv4"
      },
      "tag": "directIPv4"
    },
    {
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIPv6"
      },
      "tag": "directIPv6"
    },
    {
      "protocol": "dns",
      "settings": {
        "nonIPQuery": "drop"
      },
      "tag": "dns-out"
    },
    {
      "protocol": "blackhole",
      "settings": {
        "response": {
          "type": "http"
        }
      },
      "tag": "block"
    },
    {
      "protocol": "socks",
      "settings": {
        "servers": [
          {
            "address": "127.0.0.1",
            "port": 40000,
            "users": []
          }
        ]
      },
      "tag": "warp"
    }
  ],
  "policy": {
    "levels": {
      "1": {
        "statsUserUplink": true,
        "statsUserDownlink": true,
        "statsUserOnline": true
      },
      "0": {
        "handshake": 4,
        "connIdle": 300,
        "uplinkOnly": 2,
        "downlinkOnly": 5,
        "statsUserUplink": true,
        "statsUserDownlink": true,
        "statsUserOnline": true
      }
    },
    "system": {
      "statsInboundDownlink": true,
      "statsInboundUplink": true,
      "statsOutboundDownlink": true,
      "statsOutboundUplink": true
    }
  },
  "api": {
    "tag": "api",
    "services": [
      "HandlerService",
      "StatsService",
      "ReflectionService"
    ]
  },
  "fakedns": [
    {
      "ipPool": "198.18.0.0/16",
      "poolSize": 512
    },
    {
      "ipPool" : "fc00::/64",
      "poolSize" : 512
    }
  ],
  "stats": {},
  "reverse": null,
  "transport": null,
  "observatory": null,
  "burstObservatory": null
}
